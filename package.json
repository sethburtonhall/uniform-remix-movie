{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css --watch",
    "postinstall": "remix setup node",
    "manifest": "uniform optimize manifest download --output ./app/lib/intentManifest.json"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.11.1",
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@uniformdev/canvas": "^14.0.0",
    "@uniformdev/canvas-contentful": "^14.0.0",
    "@uniformdev/canvas-react": "^14.0.0",
    "@uniformdev/cli": "^14.0.0",
    "@uniformdev/optimize-tracker": "^14.0.0",
    "@uniformdev/optimize-tracker-browser": "^14.0.0",
    "@uniformdev/optimize-tracker-react": "^14.0.0",
    "contentful": "^9.1.6",
    "dotenv": "^15.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
